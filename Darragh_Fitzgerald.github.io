/*Code For Page One*/
.main {
  background-color: #333333;
}

img {
  max-width: 100%;
}

#nav {
  top: 0;
  position: fixed;
  width: 100%;
  background-color: rgb(39, 39, 39);
  z-index: 99999;
}

.topicons {
  display: inline-block;
}

.Toppage {
  height: 800px;
}
.logo {
  display: inline-block;
  position: absolute;
  left: 0;
  top: 0;
  padding: 20px;
  /* background: pink;s */
}

.button {
  display: in inline-block;
  position: relative;
  right: 20px;
  top: -20px;
  font-size: -webkit-xxx-large;
  margin-bottom: 50px;
  /* margin: 20px; */
  /* padding: 20px; */
  cursor: pointer;
}

.right-nav {
  float: right;
  color: yellow;
}

#burger-button {
  padding: 20px;
}

.burger-btn {
  display: in inline-block;
  position: fixed;
  right: 0;
  top: 0;
  margin: 20px;
  padding: 20px;
}
.container {
  display: inline-block;
  cursor: pointer;
  position: relative;
  z-index: 5;
}

.bar1,
.bar2,
.bar3 {
  width: 35px;
  height: 5px;
  background-color: #333;
  margin: 6px 0;
  transition: 0.4s;
}

.change .bar1 {
  -webkit-transform: rotate(-45deg) translate(-9px, 6px);
  transform: rotate(-45deg) translate(-9px, 6px);
}

.change .bar2 {
  opacity: 0;
}

.change .bar3 {
  -webkit-transform: rotate(45deg) translate(-8px, -8px);
  transform: rotate(45deg) translate(-8px, -8px);
}

.missionstatement {
  display: inline-block;
}

.test {
  background: rgba(243, 116, 138, 0.507);
  /* border: 5px solid purple; */
}

#sidebar {
  position: fixed;
  z-index: 10;
  top: 0;
  padding-top: 200px;
  background: #272824;
  right: 0;
  border: 0px solid red;
  color: yellow;
  min-height: 100vh;
}

#sidebar > ul.home-menu{
  list-style-type: none;
  padding: 50px;
}

#sidebar > ul.home-menu > li {
  margin-top: 20px;
}

